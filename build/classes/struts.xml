<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
  <constant name="struts.devMode" value="true" />
 	
  <package name="default" namespace="/" extends="struts-default">
		
		<action name="addAdminAction" 
			class="action.AdminAction" method="addAdmin" >
		    <result name="success">regist.jsp</result>
		</action>
	
		<action name="listAdminAction"
			class="action.AdminAction" method="listAdmin" >
		    <result name="success">regist.jsp</result>
		</action>	
		
		<action name="QueryAction"
			class="action.QueryAction" method="query" >
		    <result name="SUCCESS">query.jsp</result>
		</action>	
		

  </package>	
</struts>